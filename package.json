{
  "name": "senior-backend-graphql",
  "version": "1.0.0",
  "description": "graphql and simplyrets take-home assignment for interview candidates.",
  "main": "index.js",
  "engines": {
    "node": "^16"
  },
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "dev": "tsc-watch --onSuccess \"npm run start:server\"",
    "generate": "graphql-codegen --config codegen.yml",
    "format": "prettier --write src",
    "lint": "tslint src/**/*.ts",
    "pretest": "npm run lint",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "start": "npm run compile && npm run start:server",
    "start:server": "node --es-module-specifier-resolution=node ./dist/index.js"
  },
  "dependencies": {
    "@apollo/server": "^4.2.2",
    "apollo-server": "3.11.1",
    "axios": "^1.2.0",
    "dotenv": "^16.0.3",
    "graphql": "16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.15.0",
    "@graphql-codegen/typescript": "^2.8.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.8",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "jest": "27.5.1",
    "prettier": "^2.8.0",
    "supertest": "^6.3.2",
    "ts-jest": "^29.0.3",
    "tsc-watch": "^5.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  },
  "license": "UNLICENSED",
  "author": "Side Inc.",
  "repository": "https://github.com/reside-eng/take-home-assignment-graphql-simplyrets.git"
}
